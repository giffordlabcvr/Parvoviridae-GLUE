delete project parvoviridae
create project parvoviridae "A GLUE project for the Parvoviridae" --minVersion 0.1.66

# Create the database schema
schema-project parvoviridae
  run file glue/core/parvoSchemaExtensions.glue
  run file glue/core/parvoSchemaExtensionsPaleovirus.glue
  exit


# Load project settings
project parvoviridae
  run file glue/core/parvoProjectSettings.glue
  exit


# Build the core project for family Parvoviridae (Genus master references only)
project parvoviridae
  run file glue/core/buildParvoCoreProject.glue
  exit


# Build genus-level extensions (Each includes representatives of all species within the genus)
project parvoviridae

  # Parvovirinae
  run file glue/genus/ave/buildAveProject.glue
  run file glue/genus/boca/buildBocaProject.glue
  run file glue/genus/erythyro/buildErythyroProject.glue
  run file glue/genus/dependo/buildDependoProject.glue
  run file glue/genus/copi/buildCopiProject.glue
  run file glue/genus/tetra/buildTetraProject.glue
  run file glue/genus/amdo/buildAmdoProject.glue
  run file glue/genus/proto/buildProtoProject.glue
      
  # Hamaparvovirinae
  run file glue/genus/chaphama/buildChaphamaProject.glue
  run file glue/genus/ichthama/buildIchthamaProject.glue

exit


# Build paleovirus extensions for each genus that has EVE representatives
project parvoviridae

  # Parvovirinae
  run file glue/eve/amdo/buildAmdoPaleoProject.glue
  run file glue/eve/erythyro/buildErythyroPaleoProject.glue
  run file glue/eve/proto/buildProtoPaleoProject.glue
  run file glue/eve/dependo/buildDependoPaleoProject.glue
  
  # Hamaparvovirinae
  run file glue/eve/ichthama/buildIchthamaPaleoProject.glue
  
  # Create the root phylogeny with EVEs included
  #run file glue/core/epvCreateRootPhylogenies.glue
  
exit

