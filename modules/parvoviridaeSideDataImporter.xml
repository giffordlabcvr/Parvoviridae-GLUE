<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<textFilePopulator>
    <columnDelimiterRegex>\t</columnDelimiterRegex>
    
    <textFileColumn>
        <identifier>true</identifier>
        <header>accession-ID</header>
        <property>sequenceID</property>
    </textFileColumn>

    <textFileColumn>
        <header>virus_name</header>
        <property>name</property>
    </textFileColumn>

    <textFileColumn>
        <header>virus_full_name</header>
        <property>full_name</property>
    </textFileColumn>

    <textFileColumn>
        <header>virus_genus</header>
        <property>genus</property>
    </textFileColumn>

    <textFileColumn>
        <header>virus_clade</header>
        <property>clade</property>
    </textFileColumn>

    <textFileColumn>
        <header>virus_subfamily</header>
        <property>subfamily</property>
    </textFileColumn>

    <textFileColumn>
        <header>host_species</header>
        <property>host_sci_name</property>
    </textFileColumn>

    <skipMissing>true</skipMissing>

</textFilePopulator>
