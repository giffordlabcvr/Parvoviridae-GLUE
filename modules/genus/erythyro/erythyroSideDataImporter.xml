<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<textFilePopulator>
    <columnDelimiterRegex>\t</columnDelimiterRegex>
    
    <textFileColumn>
        <identifier>true</identifier>
        <header>accession-ID</header>
        <property>sequenceID</property>
    </textFileColumn>

    <textFileColumn>
        <header>lab_construct</header>
        <property>gb_lab_construct</property>
    </textFileColumn>

    <textFileColumn>
        <header>virus_name</header>
        <property>name</property>
    </textFileColumn>

    <textFileColumn>
        <header>virus_full_name</header>
        <property>full_name</property>
    </textFileColumn>

    <textFileColumn>
        <header>virus_genus</header>
        <property>genus</property>
    </textFileColumn>

    <textFileColumn>
        <header>assign_clade</header>
        <property>assign_clade</property>
    </textFileColumn>

    <textFileColumn>
        <header>assign_subclade</header>
        <property>assign_subclade</property>
    </textFileColumn>

    <textFileColumn>
        <header>virus_clade_ns</header>
        <property>clade_ns</property>
    </textFileColumn>

    <textFileColumn>
        <header>virus_subclade_ns</header>
        <property>subclade_ns</property>
    </textFileColumn>

    <textFileColumn>
        <header>virus_clade_vp</header>
        <property>clade_vp</property>
    </textFileColumn>

    <textFileColumn>
        <header>virus_subclade_vp</header>
        <property>subclade_vp</property>
    </textFileColumn>
 

    <skipMissing>true</skipMissing>

</textFilePopulator>
