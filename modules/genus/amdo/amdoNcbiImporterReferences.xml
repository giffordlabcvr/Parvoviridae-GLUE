  <sequenceFormat>GENBANK_XML</sequenceFormat>
  <sourceName>ncbi-refseqs-amdo</sourceName>
  <specificPrimaryAccessions>

	<!-- <primaryAccession>NC_001662</primaryAccession> Master refererence ADV is in core project -->
