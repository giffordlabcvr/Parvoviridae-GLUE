	<primaryAccession>NC_034445</primaryAccession> <!-- Skunk amdoparvovirus (SKAV) -->
	<primaryAccession>NC_031751</primaryAccession> <!-- Red panda amdoparvovirus (RpAPV) -->
	<primaryAccession>MT770848</primaryAccession>  <!-- Labrador amdoparvovirus (LAPV) -->



  </specificPrimaryAccessions>  
  <sequenceIdField>PRIMARY_ACCESSION</sequenceIdField>
</ncbiImporter>

