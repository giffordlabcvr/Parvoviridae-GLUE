  
  # delete previous extension
  run file glue/eve/amdo/deletePreviousExtensionBuild.glue
 

  
  # Import endogenous parvoviral element (EPV) sequences
  run file glue/eve/amdo/epvAmdoImportSources.glue
  
  # Import data for EVE sequences
  run file glue/eve/amdo/epvAmdoImportData.glue
  
  # Declare references 
  run file glue/eve/amdo/epvAmdoReferences.glue



  # Set up constrained alignment tree for the EPV subclades
  run file glue/eve/amdo/epvAmdoAlignmentTree.glue

  # Import genus level alignments
  run file glue/eve/amdo/epvAmdoAlignmentsRoot.glue


  # Record feature coverage in root and clade-level alignments
  run file glue/eve/amdo/epvAmdoRecordFeaturePresence.glue


  
  # Create the ortholog (tip) alignments in the constrained alignment tree
  run script glue/eve/amdo/epvAmdoDemoteOrthologsToTipAlignments.js

  # Create tip-level phylogenies
  run script glue/eve/amdo/epvAmdoCreateTipPhylogenies.js



  # Create root-level phylogenies for major genes
  run file glue/eve/amdo/epvAmdoCreateRootPhylogenies.glue

  # Export annotations
  run file glue/eve/amdo/epvAmdoExportRootPhylogenyAnnotations.glue



  # Validate
  validate
