  
  # delete previous extension
  run file glue/build/eve/amdo/deletePreviousExtensionBuild.glue
 

  
  # Import endogenous parvoviral element (EPV) sequences
  run file glue/build/eve/amdo/epvAmdoImportSources.glue
  
  # Import data for EVE sequences
  run file glue/build/eve/amdo/epvAmdoImportData.glue
  
  # Declare references 
  run file glue/build/eve/amdo/epvAmdoReferences.glue



  # Set up constrained alignment tree for the EPV subclades
  run file glue/build/eve/amdo/epvAmdoAlignmentTree.glue

  # Import genus level alignments
  run file glue/build/eve/amdo/epvAmdoAlignmentsRoot.glue


  # Record feature coverage in root and clade-level alignments
  run file glue/build/eve/amdo/epvAmdoRecordFeaturePresence.glue



  # Validate
  validate
