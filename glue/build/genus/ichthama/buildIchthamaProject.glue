
  # delete previous extension build
  run file glue/genus/ichthama/deleteIchthamaExtension.glue

  # create modules
  run file glue/genus/ichthama/ichthamaModules.glue

  # Import sources 
  import source sources/genus/ichthama/ncbi-refseqs-ichthama
  
  # Import data 
  run file glue/genus/ichthama/ichthamaLoadVirusData.glue

  # Declare reference sequences
  run file glue/genus/ichthama/ichthamaReferences.glue

  # Import alignments 
  run file glue/genus/ichthama/ichthamaAlignments.glue

  # Define constrained alignment tree 
  run file glue/genus/ichthama/ichthamaAlignmentTree.glue

  # Record feature coverage in alignments
  run file glue/genus/ichthama/ichthamaRecordFeaturePresence.glue

  # Validate
  validate

