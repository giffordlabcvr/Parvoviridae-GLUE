
  # delete previous extension build
  run file glue/genus/chaphama/deleteChaphamaExtension.glue

  # create modules
  run file glue/genus/chaphama/chaphamaModules.glue

  # Import sources 
  import source sources/genus/chaphama/ncbi-refseqs-chaphama
  import source sources/genus/chaphama/fasta-refseqs-chaphama
  
  # Import data 
  run file glue/genus/chaphama/chaphamaLoadVirusData.glue

  # Declare reference sequences
  run file glue/genus/chaphama/chaphamaReferences.glue

  # Import alignments 
  run file glue/genus/chaphama/chaphamaAlignments.glue

  # Define constrained alignment tree 
  run file glue/genus/chaphama/chaphamaAlignmentTree.glue

  # Record feature coverage in alignments
  run file glue/genus/chaphama/chaphamaRecordFeaturePresence.glue

  # Validate
  validate
