
project aav

  # Clean up previous extension  build
  run file glue/extension/aavExtensionDeletePreviousBuild.glue

  # Import the curated set of GenBank sequences 
  import source sources/ncbi-curated/

  # Extend constrained alignment tree
  run script glue/extension/aavExtensionLoadIsolateData.js

  # Extract data from the GenBank files
  module aavGenbankXmlPopulator populate
  module aavSideDataImporter populate -f tabular/curated/curated-side-data.tsv

  # Import alignments
  run file glue/extension/aavExtensionAlignments.glue
  
  # Extend constrained alignment tree
  run file glue/extension/aavExtensionAlignmentTree.glue
  
  # Record feature presence
  run file glue/extension/aavRecordFeaturePresence.glue
  
  # Create extension phylogenies
  run file glue/extension/aavCreateFirstPassPhylogenies.glue

  exit
  
