	create feature whole_genome "Whole genome"
    
    	feature whole_genome set metatag INFORMATIONAL true
	
    	create feature 5UTR -p whole_genome "5' UTR"

    	create feature NS -p whole_genome "Replicase protein"
        feature NS set field displayName "Replicase"
        feature NS set metatag OWN_CODON_NUMBERING true
        feature NS set metatag INCLUDE_IN_SUMMARY true
        feature NS set metatag CODES_AMINO_ACIDS true
        feature NS set metatag CODON_LABELER_MODULE kuiken2006CodonLabeler
 
    	create feature M -p whole_genome "Mid-ORF"
        feature M set field displayName "Mid-ORF"
        feature M set metatag OWN_CODON_NUMBERING true
        feature M set metatag INCLUDE_IN_SUMMARY true
        feature M set metatag CODES_AMINO_ACIDS true
        feature M set metatag CODON_LABELER_MODULE kuiken2006CodonLabeler

		create feature VP -p whole_genome "Capsid protein"
        feature VP set field displayName "Capsid"
        feature VP set metatag OWN_CODON_NUMBERING true
        feature VP set metatag INCLUDE_IN_SUMMARY true
        feature VP set metatag CODES_AMINO_ACIDS true
        feature VP set metatag CODON_LABELER_MODULE kuiken2006CodonLabeler
    
		create feature 3UTR "3' UTR"

