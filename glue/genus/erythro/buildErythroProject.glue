
  # delete previous extension build
  run file glue/genus/erythro/deleteErythroExtension.glue

  # create modules
  run file glue/genus/erythro/erythroModules.glue

  # Import sources 
  import source sources/genus/erythro/ncbi-refseqs-erythro
  import source sources/genus/erythro/fasta-refseqs-erythro

  # Import data 
  run file glue/genus/erythro/erythroImportVirusData.glue

  # Declare reference sequences
  run file glue/genus/erythro/erythroReferences.glue

  # Import alignments 
  run file glue/genus/erythro/erythroAlignments.glue

  # Define constrained alignment tree 
  run file glue/genus/erythro/erythroAlignmentTree.glue

  # Record feature coverage in alignments
  run file glue/genus/erythro/erythroRecordFeaturePresence.glue

  # Create phylogenies
  run file glue/genus/erythro/erythroCreateRootPhylogenies.glue

  # Validate
  validate
