
  # delete previous extension build
  run file glue/genus/proto/deleteProtoExtension.glue

  # create modules
  run file glue/genus/proto/protoModules.glue

  # Import sources 
  import source sources/ncbi-refseqs-proto

  # Import data 
  run file glue/genus/proto/protoLoadVirusData.glue

  # Declare reference sequences
  run file glue/genus/proto/protoReferences.glue

  # Add feature locations to reference sequences
  module featureLocationTextToGlue transform tabular/genus/proto-refseq-feature-locations.tsv -r

  # Import alignments 
  run file glue/genus/proto/protoAlignments.glue

  # Define constrained alignment tree 
  run file glue/genus/proto/protoAlignmentTree.glue

  # Record feature coverage in alignments
  run file glue/genus/proto/protoRecordFeaturePresence.glue

  # Create phylogenies
  run file glue/genus/proto/protoCreateRootPhylogenies.glue

  # Validate
  validate
