  # Clean up from any previous run of this file
  delete alignment AL_UNC_PROTO_ROOT_NS
  delete alignment AL_UNC_PROTO_ROOT_VP
  
  delete module fastaAlignmentExporter
  delete module protoTreeAlignmentImporterNucs
  delete module raxmlPhylogenyGenerator
  delete module phyloUtility
  delete module protoFigTreeAnnotationExporter
  
  # Create all the modules we need
  create module -f modules/core/fastaAlignmentExporter.xml
  create module -f modules/genus/proto/protoTreeAlignmentImporterNucs.xml
  create module -t raxmlPhylogenyGenerator
  create module -t phyloUtility
  create module -f modules/genus/proto/protoFigTreeAnnotationExporter.xml

  # Export the gene alignments 
  module fastaAlignmentExporter
    export AL_MASTER_Proto -r REF_Proto_MASTER_CPV -f Rep78 -w "fLocNotes.featureLoc.feature.name = 'Rep78' and fLocNotes.ref_nt_coverage_pct >= 60"  -c -e -o alignments/tree/proto-root-rep78.aln.fna
 	export AL_MASTER_Proto -r REF_Proto_MASTER_CPV -f VP1 -w "fLocNotes.featureLoc.feature.name = 'VP1' and fLocNotes.ref_nt_coverage_pct >= 60"  -c -e -o alignments/tree/proto-root-vp1.aln.fna
 	exit

  # Import the alignments to GLUE
  module protoTreeAlignmentImporterNucs import AL_UNC_PROTO_ROOT_NS -f alignments/tree/proto-root-rep78.aln.fna
  module protoTreeAlignmentImporterNucs import AL_UNC_PROTO_ROOT_VP -f alignments/tree/proto-root-vp1.aln.fna

  # Create the phylogenies
  module raxmlPhylogenyGenerator
    generate nucleotide phylogeny AL_UNC_PROTO_ROOT_NS -a -o trees/virus-root/proto-root-rep78.export_nucs.tre NEWICK_BOOTSTRAPS 
    generate nucleotide phylogeny AL_UNC_PROTO_ROOT_VP -a -o trees/virus-root/proto-root-vp1.export_nucs.tre NEWICK_BOOTSTRAPS 
    exit

  # Re-root phylogeny 
  module phyloUtility 

	reroot-phylogeny \
      --inputFile trees/virus-root/proto-root-rep78.export_nucs.tre NEWICK_BOOTSTRAPS \
      --midpoint \
      --outputFile trees/virus-root/proto-root-rep78.export_nucs-MidpointRerooted.tree NEWICK_BOOTSTRAPS

	reroot-phylogeny \
      --inputFile trees/virus-root/proto-root-vp1.export_nucs.tre NEWICK_BOOTSTRAPS \
      --midpoint \
      --outputFile trees/virus-root/proto-root-vp1.export_nucs-MidpointRerooted.tree NEWICK_BOOTSTRAPS

    exit

  # Export annotations
  module protoFigTreeAnnotationExporter 
    export figtree-annotation AL_UNC_PROTO_ROOT_NS -f trees/virus-root/proto-root-rep78.figtree-annotations.tsv
    export figtree-annotation AL_UNC_PROTO_ROOT_VP -f trees/virus-root/proto-root-vp1.figtree-annotations.tsv

  exit


