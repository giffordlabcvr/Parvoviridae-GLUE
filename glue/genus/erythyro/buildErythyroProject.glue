
  # delete previous extension build
  run file glue/genus/erythyro/deleteErythyroExtension.glue

  # create modules
  run file glue/genus/erythyro/erythyroModules.glue

  # Import sources 
  import source sources/genus/erythyro/ncbi-refseqs-erythyro
  import source sources/genus/erythyro/fasta-refseqs-erythyro

  # Import data 
  run file glue/genus/erythyro/erythyroImportVirusData.glue

  # Declare reference sequences
  run file glue/genus/erythyro/erythyroReferences.glue

  # Import alignments 
  run file glue/genus/erythyro/erythyroAlignments.glue

  # Define constrained alignment tree 
  run file glue/genus/erythyro/erythyroAlignmentTree.glue

  # Record feature coverage in alignments
  run file glue/genus/erythyro/erythyroRecordFeaturePresence.glue

  # Create phylogenies
  run file glue/genus/erythyro/erythyroCreateRootPhylogenies.glue

  # Validate
  validate
