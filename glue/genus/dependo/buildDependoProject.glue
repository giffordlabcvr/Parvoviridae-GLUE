
  # create modules
  run file glue/genus/dependo/dependoModules.glue

  # Import sources 
  import source sources/ncbi-refseqs-dependo

  # Import data 
  run file glue/genus/dependo/dependoLoadVirusData.glue

  # Declare reference sequences
  run file glue/genus/dependo/dependoReferences.glue

  # Add feature locations to reference sequences
  module featureLocationTextToGlue transform tabular/genus/dependo-refseq-feature-locations.tsv -r

  # Import alignments 
  run file glue/genus/dependo/dependoAlignments.glue

  # Define constrained alignment tree 
  run file glue/genus/dependo/dependoAlignmentTree.glue

  # Record feature coverage in alignments
  run file glue/genus/dependo/dependoRecordFeaturePresence.glue

  # Create phylogenies
  #run file glue/genus/dependo/dependoCreateRootPhylogenies.glue

  # Validate
  validate
