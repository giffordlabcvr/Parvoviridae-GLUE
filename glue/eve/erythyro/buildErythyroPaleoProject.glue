  
  # delete previous extension
  run file glue/eve/erythyro/deletePreviousExtensionBuild.glue
  
  
  
  # Import endogenous parvoviral element (EPV) sequences
  run file glue/eve/erythyro/epvErythyroImportSources.glue
  
  # Import data for EVE sequences
  run file glue/eve/erythyro/epvErythyroImportData.glue
  
  # Declare references 
  run file glue/eve/erythyro/epvErythyroReferences.glue



  # Set up constrained alignment tree for the EPV subclades
  run file glue/eve/erythyro/epvErythyroAlignmentTree.glue

  # Import genus level alignments
  run file glue/eve/erythyro/epvErythyroAlignmentsRoot.glue


  # Record feature coverage in root and clade-level alignments
  run file glue/eve/erythyro/epvErythyroRecordFeaturePresence.glue


  
  # Create the ortholog (tip) alignments in the constrained alignment tree
  run script glue/eve/erythyro/epvErythyroDemoteOrthologsToTipAlignments.js

  # Create tip-level phylogenies
  run script glue/eve/erythyro/epvErythyroCreateTipPhylogenies.js



  # Create root-level phylogenies for major genes
  run file glue/eve/erythyro/epvErythyroCreateRootPhylogenies.glue

  # Export annotations
  run file glue/eve/erythyro/epvErythyroExportRootPhylogenyAnnotations.glue



  # Validate
  validate
