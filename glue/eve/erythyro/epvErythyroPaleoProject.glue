  
  # delete previous extension
  run file glue/eve/amdo/deletePreviousExtensionBuild.glue

  # create modules
  run file glue/eve/amdo/epvErythryoModules.glue
  
  # Import endogenous parvoviral element (EPV) sequences
  run file glue/eve/amdo/epvErythryoImportSources.glue
  
  # Import data for EVE sequences
  run file glue/eve/amdo/epvErythryoImportData.glue
  
  # Declare references 
  run file glue/eve/amdo/epvErythryoReferences.glue

  # Import genus level alignments and define constrained alignment tree
  run file glue/eve/amdo/epvErythryoAlignmentsRoot.glue
  
  # Set up constrained alignment tree for the Dependoparvovirus EPV subclades
  run file glue/eve/amdo/epvErythryoAlignmentTree.glue

  # Record feature coverage in root and clade-level alignments
  run file glue/eve/amdo/epvErythryoRecordFeaturePresence.glue

  # Create root-level phylogenies for major genes
  run file glue/eve/amdo/epvErythryoCreateRootPhylogenies.glue

  # Export annotations
  #run file glue/eve/amdo/epvErythryoExportRootPhylogenyAnnotations.glue

  # Create the ortholog (tip) alignments in the constrained alignment tree
  #run script glue/eve/amdo/epvErythryoDemoteOrthologsToTipAlignments.js

  # Create tip-level phylogenies
  #run script glue/eve/amdo/epvErythryoCreateTipPhylogenies.js

  # Validate
  validate
