  
  # delete previous extension
  #run file glue/eve/dependo/epvDeletePreviousExtensionBuild.glue

  # create modules
  run file glue/eve/dependo/epvModules.glue
  
  # Import endogenous parvoviral element (EPV) sequences
  run file glue/eve/dependo/epvImportSources.glue
  
  # Import data for EVE sequences
  run file glue/eve/dependo/epvImportData.glue
  
  # Declare references 
  run file glue/eve/dependo/epvReferences.glue

  # Import genus level alignments and define constrained alignment tree
  run file glue/eve/dependo/epvAlignmentsRoot.glue
  
  # Set up constrained alignment tree for the Dependoparvovirus EPV subclades
  run file glue/eve/dependo/epvAlignmentTree.glue
  run file glue/eve/dependo/epvAlignmentTreeDigs.glue

  # Record feature coverage in root and clade-level alignments
  run file glue/eve/dependo/epvRecordFeaturePresence.glue

  # Create root-level phylogenies for major genes
  run file glue/eve/dependo/epvCreateRootPhylogenies.glue

  # Export annotations
  run file glue/eve/dependo/epvExportRootPhylogenyAnnotations.glue

  # Create the ortholog (tip) alignments in the constrained alignment tree
  run script glue/eve/dependo/epvDemoteOrthologsToTipAlignments.js

  # Create tip-level phylogenies
  run script glue/eve/dependo/epvCreateTipPhylogenies.js

  # Validate
  validate
