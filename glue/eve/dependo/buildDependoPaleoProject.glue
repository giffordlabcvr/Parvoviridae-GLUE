  
  # delete previous extension
  #run file glue/eve/dependo/epvDependoDeletePreviousExtensionBuild.glue


  # create modules
  run file glue/eve/dependo/epvDependoModules.glue
  
  # Import endogenous parvoviral element (EPV) sequences
  run file glue/eve/dependo/epvDependoImportSources.glue
  
  # Import data for EVE sequences
  run file glue/eve/dependo/epvDependoImportData.glue
  
  # Declare references 
  run file glue/eve/dependo/epvDependoReferences.glue


  # Set up constrained alignment tree for the Dependoparvoviruses
  run file glue/eve/dependo/epvDependoAlignmentTree.glue

  # Import genus level alignments and define constrained alignment tree
  run file glue/eve/dependo/epvDependoAlignmentsRoot.glue
  
  # Record feature coverage in root and clade-level alignments
  run file glue/eve/dependo/epvDependoRecordFeaturePresence.glue


  # Set up constrained alignment tree for the Dependoparvovirus EPV subclades
  run file glue/eve/dependo/epvDependoAlignmentTreeDigs.glue

  # Create the ortholog (tip) alignments in the constrained alignment tree
  run script glue/eve/dependo/epvDependoDemoteOrthologsToTipAlignments.js

  # Create tip-level phylogenies
  #run script glue/eve/dependo/epvDependoCreateTipPhylogenies.js

  # Create root-level phylogenies for major genes
  #run file glue/eve/dependo/epvDependoCreateRootPhylogenies.glue

  # Export annotations
  #run file glue/eve/dependo/epvDependoExportRootPhylogenyAnnotations.glue


  # Validate
  validate
