  
  # delete previous extension
  #run file glue/eve/erythro/epvDeletePreviousExtensionBuild.glue

  # create modules
  run file glue/eve/erythro/epvModules.glue
  
  # Import endogenous parvoviral element (EPV) sequences
  run file glue/eve/erythro/epvImportSources.glue
  
  # Import data for EVE sequences
  run file glue/eve/erythro/epvImportData.glue
  
  # Declare references 
  run file glue/eve/erythro/epvReferences.glue

  # Import genus level alignments and define constrained alignment tree
  run file glue/eve/erythro/epvAlignmentsRoot.glue
  
