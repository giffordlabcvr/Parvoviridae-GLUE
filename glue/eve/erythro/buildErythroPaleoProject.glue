  
  # delete previous extension
  run file glue/eve/erythro/deletePreviousExtensionBuild.glue
  
  
  
  # Import endogenous parvoviral element (EPV) sequences
  run file glue/eve/erythro/epvErythroImportSources.glue
  
  # Import data for EVE sequences
  run file glue/eve/erythro/epvErythroImportData.glue
  
  # Declare references 
  run file glue/eve/erythro/epvErythroReferences.glue



  # Set up constrained alignment tree for the EPV subclades
  run file glue/eve/erythro/epvErythroAlignmentTree.glue

  # Import genus level alignments
  run file glue/eve/erythro/epvErythroAlignmentsRoot.glue


  # Record feature coverage in root and clade-level alignments
  run file glue/eve/erythro/epvErythroRecordFeaturePresence.glue


  
  # Create the ortholog (tip) alignments in the constrained alignment tree
  run script glue/eve/erythro/epvErythroDemoteOrthologsToTipAlignments.js

  # Create tip-level phylogenies
  run script glue/eve/erythro/epvErythroCreateTipPhylogenies.js



  # Create root-level phylogenies for major genes
  run file glue/eve/erythro/epvErythroCreateRootPhylogenies.glue

  # Export annotations
  run file glue/eve/erythro/epvErythroExportRootPhylogenyAnnotations.glue



  # Validate
  validate
